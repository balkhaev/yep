{
	"$schema": "https://raw.githubusercontent.com/nicehash/tauri/dev/packages/tauri-cli/schema.json",
	"productName": "yep-mem",
	"version": "0.1.0",
	"identifier": "com.yep.mem",
	"build": {
		"frontendDist": "../dist",
		"devUrl": "http://localhost:1420",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "yep-mem",
				"width": 1100,
				"height": 750,
				"minWidth": 800,
				"minHeight": 600,
				"decorations": true,
				"transparent": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"shell": {
			"open": true
		},
		"updater": {
			"active": true,
			"dialog": true,
			"endpoints": [
				"https://github.com/balkhaev/yep/releases/latest/download/latest.json"
			]
		}
	}
}
